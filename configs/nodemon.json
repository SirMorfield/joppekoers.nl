{
	"restartable": "rs",
	"ignore": [
		"*.log",
		"npm-debug.log*",
		".DS_Store*",
		"node_modules/",
		".npm",
		".eslintcache",
		"users.json",
		"package-lock.json",
		"sessions/",
		"temp.*",
		"test.*"
	],
	"verbose": false,
	"execMap": {
		"js": "node"
	},
	"watch": [
		"."
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js",
	"events": {
		"start": "clear"
	}
}
