<body>
  <style>
    pre {
      outline: 1px solid #ccc;
      padding: 5px;
      margin: 5px;
    }

    .string {
      color: green;
    }

    .number {
      color: darkorange;
    }

    .boolean {
      color: blue;
    }

    .null {
      color: magenta;
    }

    .key {
      color: red;
    }
  </style>

  <% const parsedStations = JSON.parse(stations) %>
  <% for(const station in parsedStations) { %>
  <textarea cols="60" rows="30" class="station"><%= station %>: <%= JSON.stringify(parsedStations[station], null, 2) %></textarea>
  <%}%>

