<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>deletthis</title>
  <link rel="stylesheet" href="../deletthis/deletthis.css">
  <script src='/socket.io/socket.io.js'></script>
  <script>
    var socket = io();
    const next = () => {
      socket.emit('reqNewImg');
      socket.on('resNewImg', function (newImg) {
        document.getElementById('img').src = newImg;
      })
    }
  </script>
</head>

<body>
  <div id="box">
    <a class="prev" onclick="prev()">&#10094;</a>
    <a class="next" onclick="next()">&#10095;</a>
    <div id="imgContainer">
      <%- img %>
      <!-- '<img src=\"../deletthis/img/' + img + '\" id="img">' -->
    </div>

    <div id="uploadBox">
      <form class="fileupload" action="upload" method="post" enctype="multipart/form-data">
        <input type="file" name="upfile" value="">
        <input type="submit" />
      </form>
    </div>

  </div>
</body>

</html>
